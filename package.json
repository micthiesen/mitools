{
  "name": "@micthiesen/mitools",
  "version": "1.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/micthiesen/mitools"
  },
  "type": "module",
  "exports": {
    "./collections": "./dist/collections/index.js",
    "./config": "./dist/config/index.js",
    "./docstore": "./dist/persistence/docstore.js",
    "./entities": "./dist/persistence/entities.js",
    "./logging": "./dist/logging/index.js",
    "./pushover": "./dist/services/pushover.js",
    "./strings": "./dist/utils/strings.js",
    "./types": "./dist/types/index.js"
  },
  "scripts": {
    "build": "rm -fr ./dist && tsc",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "format": "biome format .",
    "check": "biome check .",
    "test": "vitest .",
    "publish": "pnpm build && pnpm publish --access public"
  },
  "dependencies": {
    "better-sqlite3": "^11.5.0",
    "cbor": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@types/better-sqlite3": "^7.6.11",
    "@types/node": "^20.14.12",
    "tsx": "^4.7.1",
    "typescript": "^5.0.4",
    "vitest": "^2.1.3"
  },
  "engines": {
    "node": "24.13.0",
    "pnpm": "10.28.2"
  },
  "packageManager": "pnpm@10.28.2",
  "files": ["dist"],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
